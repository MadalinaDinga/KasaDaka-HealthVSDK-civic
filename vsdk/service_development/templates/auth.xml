{% extends "base.xml" %}
{% block content%}


<form id="submit_form">
<block>
	{# include the variables in the dictionary 'pass_on_variables' as POST variables #}
	{% for key, value in pass_on_variables.items %}
	<assign name="{{ key }}" expr="'{{ value }}'"/>
	{% endfor %}
	<submit next="{{ redirect_url }}" method="post" namelist=" {% for key, value in pass_on_variables.items %} {{ key }} {% endfor %}"/>
</block>
</form>

{% endblock %}
